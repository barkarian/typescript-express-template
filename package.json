{
  "name": "typescriptExpressTemplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "helloWorld": "ts-node -r tsconfig-paths/register ./src/helloWorld.ts",
    "start:dev:watch": "nodemon -r tsconfig-paths/register -x ts-node-esm src/server.ts",
    "start:dev": "ts-node -r tsconfig-paths/register ./src/server.ts",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/server.js",
    "build": "tsc",
    "test": "jest",
    "eslint": "eslint . --ext .ts",
    "lint:fix": "npm run eslint -- --fix",
    "coverage": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\"",
    "lint-fix": "npm run lint -- --fix",
    "migrations:gen": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migrations:run": "ts-node ./node_modules/.bin/typeorm migration:run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.6.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typeorm": "^0.3.11",
    "uuidv4": "^6.2.13"
  }
}
